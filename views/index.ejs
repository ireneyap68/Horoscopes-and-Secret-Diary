<div class="container">
  <div class="row">
    <div class="col s12">
      <h2>Homepage</h2>
      <h3><%= moment(Date()).format(shortDateFormat) %></h3>
    </div>
  </div>
  <div class="row equal-col">
    <% horos.forEach(function(entry) {%>
    <div class="col s12 m6 l4">
      <div class="card">
        <div class="card-image">
          <img src="/assets/img/<%=entry%>.jpg">
        </div>
        <div class="card-content">
          <span class="card-title activator grey-text text-darken-4" style="text-transform: capitalize;"><%=entry%><i class="material-icons right">Details</i></span>
          <p><%=horoscopes[entry].date_range%></p>
        </div>
        <div class="card-reveal">
          <span class="card-title grey-text text-darken-4" style="text-transform: capitalize;"><%=entry%><i class="material-icons right">close</i></span>
          <p><%=horoscopes[entry].description%></p>
          <p>Lucky number: <%= horoscopes[entry].lucky_number %></p>
          <p>Lucky color : <%= horoscopes[entry].color %></p>
          <p>Mood of the day: <%= horoscopes[entry].mood %></p>
          <p><a href="/quote?horo=<%=entry%>">Secret of the day</a></p>
        </div>
      </div>
    </div>
    <%});%>
  </div>
</div>

